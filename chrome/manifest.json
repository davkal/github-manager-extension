{
  "name": "Github Manager Extension",
  "short_name": "Github BMI",
  "version": "0.1.1",
  "description": "Shows bug metrics on the issues page",
  "icons": { "128": "icon.png" },
  "permissions": [
    "storage"
  ],
  "content_scripts": [{
    "matches": [
      "https://github.com/*"
    ],
    "js": ["vendors.js", "inject-bundle.js"]
  }],
  "manifest_version": 2
}
